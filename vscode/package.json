{
  "name": "vue-slides",
  "displayName": "VueSlides",
  "description": "VueSlides Language Support",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:vue-slides"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "vue-slides",
        "aliases": [
          "VueSlides",
          "vue-slides",
          "vueslides"
        ],
        "extensions": [
          ".vue-slides",
          ".slides"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vue-slides",
        "scopeName": "source.vue-slides",
        "path": "./syntaxes/vue-slides.tmLanguage.json"
      }
    ]
  },
  "publisher": "znck",
  "icon": "icon.png",
  "repository": "https://github.com/znck/vue-slides",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21"
  }
}
